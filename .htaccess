#The following SHOULD ONLY be present on the OLD website / OLD picadesign.com domain
#Redirect 301 / http://pica.is/

# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /pica_website/
RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /pica_website/index.php [L]
</IfModule>

# END WordPress
